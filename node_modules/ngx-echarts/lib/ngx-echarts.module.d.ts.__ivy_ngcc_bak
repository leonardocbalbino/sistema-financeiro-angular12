import { ModuleWithProviders } from '@angular/core';
import { NgxEchartsDirective, NgxEchartsConfig } from './ngx-echarts.directive';
export declare class NgxEchartsModule {
    static forRoot(config: NgxEchartsConfig): ModuleWithProviders;
    static forChild(): {
        ngModule: typeof NgxEchartsModule;
    };
}
export { NgxEchartsDirective };
